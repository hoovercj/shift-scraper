{
  "disabled": false,
  "bindings": [
    {
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */7 * * * *",
      "name": "myTimer"
    },
    {
      "name": "currentCalendar",
      "type": "blob",
      "path": "calendar/1/currentCalendar",
      "direction": "in"
    },
    {
      "name": "oldCalendar",
      "type": "blob",
      "path": "calendar/1/oldCalendar",
      "direction": "out"
    },
    {
      "name": "newCurrentCalendar",
      "type": "blob",
      "path": "calendar/1/currentCalendar",
      "direction": "out"
    },
    {
      "name": "newEvents",
      "type": "blob",
      "path": "calendar/1/newEvents",
      "direction": "out"
    },
    {
      "name": "message",
      "type": "sendGrid",
      "direction": "out"
    }
  ]
}
